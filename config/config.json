{
    "port": 8080,
    "lasso": {
        "fileWriter": {
            "url-prefix": "/static",
            "outputDir": "path:../static",
            "fingerprintsEnabled": true
        },
        "plugins": [
            {
                "plugin": "lasso-marko",
                "config": {}
            },
            {
                "plugin": "lasso-less",
                "config": {}
            }
        ],
        "bundles": [
            {
                "name": "jquery",
                "dependencies": [
                    "require jquery"
                ]
            }
        ],
        "enabledExtensions": ["production"],
        "minify": true,
        "resolveCssUrls": true,
        "bundlingEnabled": true,
        "cacheProfile": "production"
    },
    "serveStatic": {
        "maxAge": 31536000000
    }
}
